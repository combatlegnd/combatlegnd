local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Made By combatlegnd#0440", "Synapse")

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewToggle("Skip Level", "1 tap = skip level", function(state)
    if state then
         loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Doors%20%20Skip%20level"))()

    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Doors%20%20Skip%20level"))()
    end
end)


MainSection:NewToggle("Auto skip level", "1 = auto skip level", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Auto%20skip%20door"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Auto%20skip%20door"))()
    end
end)


MainSection:NewToggle("Complete breaker box minigame", "auto click lever", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Door%20Complete%20breaker%20box%20minigame"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Door%20Complete%20breaker%20box%20minigame"))()
    end
end)


MainSection:NewToggle("Figure chams % book chams", "Show u where Firure And Books u need", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Doors%20Figure%20chams%20%25%20book%20chams"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Doors%20Figure%20chams%20%25%20book%20chams"))()
    end
end)


MainSection:NewToggle("No Seek Arm", "No More annoying Long arms", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/No%20seek%20Arm"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/No%20seek%20Arm"))()
    end
end)


MainSection:NewToggle("Fullbright", "No more scare from Dark", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Doors%20Light%20full%20Bright%20u%20can%20change%20it"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Doors%20Light%20full%20Bright%20u%20can%20change%20it"))()
    end
end)


MainSection:NewToggle("Waring when mob spawn", "ToggleInfo", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Warning%20IF%20any%20mob%20spawn"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/combatlegnd/combatlegnd/main/Warning%20IF%20any%20mob%20spawn"))()
    end
end)

MainSection:NewToggle("Infinity yield", "Use noclip and fly if u want afk but will not save u from fly thing", function(state)
    if state then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end
end)

